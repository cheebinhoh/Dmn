prefix=/usr/local
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${prefix}/lib

Name: librdkafka
Description: The Apache Kafka C/C++ library
Version: 2.8.0
Requires.private: zlib libcrypto libssl libzstd liblz4 
Cflags: -I${includedir}
Libs: -L${libdir} -lrdkafka
Libs.private: -lpthread -lrt -ldl -lm
